    "babel-eslint": "^10.1.0",
    "confusing-browser-globals": "^1.0.9",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.0.1",
